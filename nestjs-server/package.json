{
  "name": "@perfect-stack/nestjs-server",
  "version": "0.0.322",
  "description": "",
  "author": "Richard Perfect",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build:app": "nest build kims-server",
    "build:lib": "nest build nestjs-server",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start kims-server",
    "start:dev": "nest start kims-server --watch",
    "start:debug": "nest start kims-server --debug --watch",
    "start:prod": "node dist/apps/kims-server/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "meta:copy:s3": "aws s3 cp --recursive ./meta/ s3://perfect-stack-demo-meta-s3/meta",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/kims-server/test/jest-e2e.json",
    "publish:github": "npm publish",
    "migrate:images": "ts-node -r tsconfig-paths/register src/bin/migrate-images.ts"
  },
  "dependencies": {
    "@aws-sdk/client-lambda": "^3.886.0",
    "@aws-sdk/client-rds": "^3.886.0",
    "@aws-sdk/client-s3": "^3.886.0",
    "@aws-sdk/client-secrets-manager": "^3.886.0",
    "@aws-sdk/s3-request-presigner": "^3.886.0",
    "@js-joda/core": "^5.6.5",
    "@js-joda/locale_en": "^4.15.1",
    "@js-joda/timezone": "^2.22.0",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/common": "^11.1.3",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.3",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.3",
    "@nestjs/swagger": "^11.2.0",
    "@types/pg": "^8.15.4",
    "aws-lambda": "^1.0.7",
    "aws-serverless-express": "^3.4.0",
    "cls-hooked": "^4.2.2",
    "fast-csv": "^5.0.2",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.2.0",
    "knex": "^3.1.0",
    "mime-types": "^3.0.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.0",
    "proj4": "^2.17.0",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.2",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.6",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.3",
    "@types/express": "^5.0.3",
    "@types/jest": "29.5.14",
    "@types/mime-types": "^3.0.1",
    "@types/multer": "^1.4.13",
    "@types/node": "^24.0.1",
    "@types/passport-jwt": "^4.0.1",
    "@types/sequelize": "^4.28.20",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "jest": "30.0.0",
    "prettier": "^3.5.3",
    "semantic-release": "^24.2.5",
    "semantic-release-monorepo": "^8.0.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.1",
    "ts-jest": "29.4.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@perfect-stack/nestjs-server(|/.*)$": "<rootDir>/libs/nestjs-server/src/$1"
    }
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "url": "https://github.com/perfect-stack/perfect-stack.git"
  },
  "main": "dist/libs/nestjs-server/index.js",
  "types": "dist/libs/nestjs-server/index.d.ts",
  "files": [
    "dist/libs/nestjs-server"
  ]
}
